// RHours.rho

new stdout(`rho:io:stdout`) in {
    new return in {
        @"RHours"!(*return) |
        for (RHours <- return) {
            RHours!("RegisterContract", "a", 1, *return) |
            for (_ <- return) {
                RHours!("LookupContract", "a", *return) |
                for (c <- return) {
                    stdout!(*c)
                }
            }
        }
    }
}
